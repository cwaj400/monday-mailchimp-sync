{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhooks/(.*)",
      "dest": "/api/webhooks/$1",
      "continue": true,
      "auth": false
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ]
} 